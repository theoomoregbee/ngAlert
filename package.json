  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:lib": "ng build alert",
    "test": "",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-css": "node-sass-chokidar projects/alert/ -o dist/alert/",
    "copy:readme": "cp ./README.md dist/alert/README.md",
    "copy:img": "cp -r ./img/ dist/alert/img",
    "packager": "npm run build:lib && npm run build-css && npm run copy:readme && npm run copy:img && cd dist/alert",
    "deploy": "ng build --prod --base-href='/ngAlert/' && cp ./dist/index.html ./dist/404.html && ./node_modules/.bin/angular-cli-ghpages",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "core-js": "^2.5.4",
    "rxjs": "~6.2.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.0",
