{
  "name": "ngalert",
  "version": "1.0.1",
  "description": "Compile and package a TypeScript library to Angular Package Format",
  "keywords": [
    "angular",
    "angular2",
    "angular4",
    "angular5",
    "notifications",
    "angular-library",
    "alert-components",
    "alert-library",
    "typescript",
    "css",
    "html"
  ],
  "author": "Theophilus Omoregbee <theo4u@ymail.com>",
  "repository": "https://github.com/theo4u/ngAlert.git",
  "homepage": "https://github.com/theo4u/ngAlert",
  "bugs": {
    "url": "https://github.com/theo4u/ngAlert/issues"
  },
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "packagr": "ng-packagr -p ng-package.json",
    "deploy": "ng build –prod --base-href='/ngAlert/' && cp ./dist/index.html ./dist/404.html && ./node_modules/.bin/angular-cli-ghpages –no-silent"
